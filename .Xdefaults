ljwm.border: (lambda (screen) \
                   ((<screen>-Size screen) \
    		     (lambda (width height) \
    		       (max (inexact->exact (round (* .005 height))) 3))))
ljwm.button1: emacs

ljwm.leftdock: 1%
ljwm.topdock: 0
ljwm.bottomdock: 2%
ljwm.rightdock: 0

ljwm.noTitle: (lambda (c) (prefix=? ((<client>-get-name c)) "xosview"))

